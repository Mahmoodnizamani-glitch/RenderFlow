{
    "annotations": {
        "list": []
    },
    "editable": true,
    "panels": [
        {
            "title": "Queue Depth",
            "type": "timeseries",
            "gridPos": {
                "h": 8,
                "w": 12,
                "x": 0,
                "y": 0
            },
            "targets": [
                {
                    "expr": "bullmq_queue_waiting{queue=\"render\"}",
                    "legendFormat": "Waiting"
                },
                {
                    "expr": "bullmq_queue_active{queue=\"render\"}",
                    "legendFormat": "Active"
                },
                {
                    "expr": "bullmq_queue_delayed{queue=\"render\"}",
                    "legendFormat": "Delayed"
                }
            ],
            "fieldConfig": {
                "defaults": {
                    "thresholds": {
                        "steps": [
                            {
                                "color": "green",
                                "value": 0
                            },
                            {
                                "color": "yellow",
                                "value": 25
                            },
                            {
                                "color": "red",
                                "value": 50
                            }
                        ]
                    }
                }
            }
        },
        {
            "title": "Processing Time (P50 / P95)",
            "type": "timeseries",
            "gridPos": {
                "h": 8,
                "w": 12,
                "x": 12,
                "y": 0
            },
            "targets": [
                {
                    "expr": "histogram_quantile(0.50, rate(render_job_duration_seconds_bucket[5m]))",
                    "legendFormat": "P50"
                },
                {
                    "expr": "histogram_quantile(0.95, rate(render_job_duration_seconds_bucket[5m]))",
                    "legendFormat": "P95"
                }
            ],
            "fieldConfig": {
                "defaults": {
                    "unit": "s"
                }
            }
        },
        {
            "title": "Job Success / Failure Rate",
            "type": "timeseries",
            "gridPos": {
                "h": 8,
                "w": 12,
                "x": 0,
                "y": 8
            },
            "targets": [
                {
                    "expr": "rate(render_jobs_completed_total[5m])",
                    "legendFormat": "Completed"
                },
                {
                    "expr": "rate(render_jobs_failed_total[5m])",
                    "legendFormat": "Failed"
                }
            ]
        },
        {
            "title": "Active Workers",
            "type": "stat",
            "gridPos": {
                "h": 4,
                "w": 6,
                "x": 12,
                "y": 8
            },
            "targets": [
                {
                    "expr": "count(up{app=\"renderflow-worker\"} == 1)",
                    "legendFormat": "Workers"
                }
            ]
        },
        {
            "title": "Worker Memory Usage",
            "type": "timeseries",
            "gridPos": {
                "h": 8,
                "w": 12,
                "x": 0,
                "y": 16
            },
            "targets": [
                {
                    "expr": "process_resident_memory_bytes{app=\"renderflow-worker\"} / 1024 / 1024",
                    "legendFormat": "{{instance}} MB"
                }
            ],
            "fieldConfig": {
                "defaults": {
                    "unit": "decmbytes"
                }
            }
        }
    ],
    "schemaVersion": 38,
    "style": "dark",
    "tags": [
        "renderflow",
        "render",
        "worker"
    ],
    "time": {
        "from": "now-6h",
        "to": "now"
    },
    "title": "RenderFlow Render Workers",
    "uid": "renderflow-render-dashboard",
    "version": 1
}